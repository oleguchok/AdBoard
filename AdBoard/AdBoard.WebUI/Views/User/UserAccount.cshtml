@model AdBoard.WebUI.Models.UserAdsViewModel

@{
    ViewBag.Title = "Your Ads";
}

<div class="page-header">
    <h1>Profile Page</h1>
</div>

<h2>Your ads:</h2>
@foreach (var p in @Model.Ads)
{
    @Html.Partial("AdSummary", p);
}

<div class="btn-group pull-right">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("UserAccount", new { page = x}))
</div>
